<body id="top" role="document" class="file">
  <%= render '_topbar.rhtml' %>
  <%= render '_navigation.rhtml' %>

  <main role="main" aria-label="Page <%=h file.full_name%>">
    <article>
      <%= file.description %>
    </article>
    <nav class="contextual">
      <%= render '_sidebar_table_of_contents.rhtml' %>
    </nav>
  </main>